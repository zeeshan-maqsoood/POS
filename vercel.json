{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/pos/(.*)", "dest": "/pos/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/pos/:path*",
      "destination": "/pos/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
